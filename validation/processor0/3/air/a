FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "3/air";
    object      a;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -1 0 0 0 0 0];

internalField   uniform 0.01;

boundaryField
{
    airLeft
    {
        type            calculated;
        value           uniform 0.01;
    }
    airBottom
    {
        type            calculated;
        value           nonuniform 
0
;
    }
    airTop
    {
        type            calculated;
        value           uniform 0.01;
    }
    frontAndBack
    {
        type            empty;
    }
    air_to_pcm
    {
        type            calculated;
        value           uniform 0.01;
    }
    procBoundary0to2
    {
        type            processor;
        value           uniform 0.01;
    }
}


